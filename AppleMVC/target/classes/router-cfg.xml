<?xml version="1.0" encoding="UTF-8"?>

<!-- 路油配置文档（请求映射） -->
<!-- returnType的可选值（有四种类型）分别是view和text,image,还有file -->
<router>

	<!-- <route path="/signIn.action" className="com.example.controller.LoginController" method="login" returnType="view"/> -->
	<route path="/AppleMVC/signIn.action" className="com.example.controller.Controller" method="login" returnType="view"/>
	<route path="/AppleMVC/exit.action" className="com.example.controller.Controller" method="exit" returnType="view"/>
	<route path="/AppleMVC/getText.action" className="com.example.controller.Controller" method="getText" returnType="text"/>
	<route path="/AppleMVC/getImage.action" className="com.example.controller.Controller" method="getImage" returnType="image"/>
	<route path="/AppleMVC/getFile.action" className="com.example.controller.Controller" method="getFile" returnType="file"/>
	
</router>